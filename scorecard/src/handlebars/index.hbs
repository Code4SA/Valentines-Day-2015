<body>

<div class="page-frame">
	<div class="layout">
		<label>Binary toggle</label>
		<div class="field">
			<div class="binary-toggle">
				<div class="btoggle"><input type="radio" name="gender" value="woman" id="gender-woman" checked="checked"> <label for="gender-woman" class="woman">Woman</label></div>
				<div class="btoggle"><input type="radio" name="gender" value="man" id="gender-man"> <label for="gender-man" class="man">Man</label></div>
			</div>
		</div>
	</div>
</div>

<!-- loading CSS images at end of document as not essential to initial page load and render -->
<link href="assets/css/images.css" rel="stylesheet" type="text/css" property="stylesheet">
<script src="assets/js/jquery.c4saValentines.min.js"></script>

